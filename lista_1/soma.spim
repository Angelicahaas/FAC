.data
    
    quebraDeLinha: .asciiz "\n"

.text


main:
    
    li $v0, 5
    syscall

    move $t0, $v0  #$t0 = $v0

    li $v0, 5
    syscall

    move $t1, $v0 # $t1 = $v0

    add $t2, $t0, $t1 # $t2 = $t1 + $t0

    move $a0, $t2 # $a0 = $t2

    li $v0, 1
    syscall

    la $a0, quebraDeLinha

